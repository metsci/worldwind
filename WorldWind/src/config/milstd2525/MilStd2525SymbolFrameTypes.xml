    <!--Clover symbol frame type.-->
    <SymbolFrameType>
        <!--
        The Clover frame is contained in the specified rectangle within the icon's graphic. This rectangle is
        expressed as a fraction of the graphic's bounding box.
        -->
        <FrameRect x="0.0625" y="0.0625" width="0.890625" height="0.890625"/>
        <!--
        The Clover frame is 1.44x the default length and height of the bounding octagon. See MIL-STD-2525C table
        VIII, page 32.
        -->
        <FrameWidthFactor>1.44</FrameWidthFactor>
        <!--
        The Clover frame is associated with the following Standard Identities: Pending, Unknown, Exercise Pending,
        Exercise Unknown.
        -->
        <AcceptedStandardIdentities>
            <StandardIdentity>P</StandardIdentity>
            <StandardIdentity>U</StandardIdentity>
            <StandardIdentity>G</StandardIdentity>
            <StandardIdentity>W</StandardIdentity>
        </AcceptedStandardIdentities>
        <!--
        The Clover frame is associated with the following Battle Dimensions: Unknown, Ground, Sea Surface, and SOF.
        -->
        <AcceptedBattleDimensions>
            <BattleDimension>Z</BattleDimension>
            <BattleDimension>G</BattleDimension>
            <BattleDimension>S</BattleDimension>
            <BattleDimension>F</BattleDimension>
        </AcceptedBattleDimensions>
    </SymbolFrameType>

